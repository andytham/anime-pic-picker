
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
    <link rel="stylesheet" type="text/css" href="/css/reset.css" />
    <link rel="stylesheet" type="text/css" href="/css/style.css" />

</head>
<body>
<% include partials/header.ejs %>
    <% for (let image of images) { %>
        <a href="<%= `/saved/${image.id}` %>">
          <img width="300px" src="<%= image.image %>" />
        </a>
        <form action="<%= `/saved/${image.id}?_method=DELETE` %>" method="POST">
          <div class="comment">
            <%= image.comment %>
          </div>
        <input type="submit" value="Delete"/>
        </form>
      <% } %>

    <a href="/">back</a>
  </body>
</html>
