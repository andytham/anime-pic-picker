
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <% for (let image of images) { %>
        <a href="<%= `/search/saved/${image.id}` %>">
          <img width="300px" src="<%= image.image %>" />
        </a>
        <form action="<%= `/search/saved/${image.id}?_method=DELETE` %>" method="POST">
          <div class="comment">
            <%= image.comment %>
          </div>
        <input type="submit" value="Delete"/>
        </form>
      <% } %>

    <a href="/">back</a>
  </body>
</html>
